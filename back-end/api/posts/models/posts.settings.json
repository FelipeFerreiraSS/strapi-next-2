{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string",
      "required": true
    },
    "Data": {
      "type": "datetime"
    },
    "imagem": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "texto": {
      "type": "richtext"
    }
  }
}
